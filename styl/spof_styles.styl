*
	margin 0
	padding 0
	outline none
body
	background-color: #F5F3F3
	font-size: 16px
	font-family: 'OpenSans', sans-serif
a
	color: #fefefe
	&:focus
		outline: 0
	&:hover
		text-decoration none
button
	border none
.video_wrapper
	margin 0 auto
	position relative
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 3px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.button
	display: block
	width 100%
	max-width 332px
	background-color: rgb(244, 55, 88);
	width: 332px;
	height: 55px;
	line-height: 55px;
	font-family: "OpenSansCondensedBold", sans-serif;
	color: rgb(255, 255, 255);
	text-transform: uppercase;
	text-align: center;
	margin 0 auto
	border-radius: 27.5px
	font-size: 18px
	&:focus
		text-decoration: none
		color: rgb(255, 255, 255);
	&:hover
		background-color: #fc607b
		text-decoration: none
		color: rgb(255, 255, 255);
h1,h2,h3,h4,h5,h6
	font-weight: normal
.general_title
	font-size: 34px;
	font-family: "OpenSansCondensedBold", sans-serif;
	color: rgb(1, 7, 38);
	text-transform: uppercase;
	line-height: 1.3;
	text-align: center;
	margin-bottom: 85px
	span
		color #f43758

@import 'spof/main'
@import 'spof/second'
@import 'spof/plan'
@import 'spof/last'
@import 'spof/footer'