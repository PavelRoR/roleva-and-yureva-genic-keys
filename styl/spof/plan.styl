#plan
	width 100%
	padding 60px 0
	background-color: #04051B
	overflow hidden
	.container
		position relative
.plan_general_title
	color #fff
#plan_img
	position: absolute
	top 170px
	right: 65px
	display block
.plan_container
	width 100%
	max-width 560px
	margin-bottom: 20px
	overflow hidden
	display: table
.plan_container_title
	width: 44px
	height 44px
	background-color: #fff
	border-radius: 50px
	position relative
	display: table-cell
	span
		position: absolute
		font-size: 34px;
		font-family: "OpenSansCondensedBold", sans-serif;
		color: rgb(255, 57, 90);
		text-transform: uppercase;
		line-height: 1
		top 0
		left 2px
.plan_container_text
	display: table-cell
	vertical-align: middle
	font-size: 20px
	color: #fff
	line-height: 1.327;
	padding-left: 15px
.plan_subtitle 
	font-size: 24px;
	font-family: "OpenSansCondensedBold", sans-serif;
	color: #fff
	text-transform: uppercase;
	line-height: 1.3;
	margin: 40px 0 25px 50px
	span
		color #f43758
.bonuses_list
	margin-left: 55px
	li
		list-style: none
		font-size: 20px
		color: #fff
		line-height: 1.375;
		margin-bottom: 15px
		padding-left: 15px
		position relative
		&:before
			content: ''
			display block
			width 10px
			height 10px
			background-color: transparent
			border-radius: 50px
			border 2px solid #ff0f47
			position: absolute
			top 10px
			left -2px